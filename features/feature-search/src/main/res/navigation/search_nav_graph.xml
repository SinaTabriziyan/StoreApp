<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search_nav_graph"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.sina.feature_search.ui.main.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_seachOrdersFragment"
            app:destination="@id/seachOrdersFragment" />
        <action
            android:id="@+id/action_searchFragment_to_searchFiltersFragment"
            app:destination="@id/searchFiltersFragment" />
    </fragment>
    <fragment
        android:id="@+id/seachOrdersFragment"
        android:name="com.sina.feature_search.ui.order.SearchOrderByFragment"
        android:label="fragment_seach_orders"
        tools:layout="@layout/fragment_search_order_by" />
    <fragment
        android:id="@+id/searchFiltersFragment"
        android:name="com.sina.feature_search.ui.filter.SearchFiltersFragment"
        android:label="fragment_search_filters"
        tools:layout="@layout/fragment_search_filters" />
</navigation>